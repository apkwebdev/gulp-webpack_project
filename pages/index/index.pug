extends ../../../#picker/templates/col3/col3.pug

block variables
  - let title = " index"
  - let template = "col3"
  - let pagename = "index"

block main
  main.main-index
    h1.main-index__title Index page
    h3.main-index__subtitle Пример использования png спрайта и его масштабирования
    .main-index__pngicon-wrap.pngicon-wrap
      .pngicon.pngicon-gears
      .pngicon.pngicon-lightbulb
      .pngicon.pngicon-rocket
    h3.main-index__subtitle Пример использования svg спрайта и его масштабирования
    .main-index__svgicon-wrap.svgicon-wrap
      svg.svgicon.svgicon-tools #[+svguse('tools')]
      svg.svgicon.svgicon-spinner #[+svguse('spinner')]
      svg.svgicon.svgicon-wrench #[+svguse('wrench')]

